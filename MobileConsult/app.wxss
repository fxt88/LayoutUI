page{
  position: relative;
  display: flex;
  flex-direction: column;
  align-items: stretch;
  font-family: PingFangSC-Regular;
  font-size: 30rpx;
  line-height: 150%;
  color: #666;
  min-height: 100%;
}

.container {
  position: relative;
  flex: 1;
  display: flex;
  flex-direction: column;
  min-height: 100%;
  padding-bottom: 100rpx;
  background-color: #f0eff5;
  box-sizing: border-box;
} 

.fullscreen {
  width: 100%;
  height: 100%;
}
.background-wrapper {
  width: 100%;
  height: 100%;
  background-color: #fff;
}
button[disabled]:not([type]) {
  background-color:rgb(85,188,250);
  color: #fff;
}